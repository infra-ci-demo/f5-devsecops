external_url 'http://{{ ansible_host }}'
gitlab_rails['time_zone'] = 'Asia/Tokyo'
gitlab_rails['initial_root_password'] = '{{ gitlab_root_password | default("password") }}'
gitlab_rails['initial_shared_runners_registration_token'] = '{{ gitlab_runner_token | default("token-AABBCC") }}'
gitlab_rails['gitlab_shell_ssh_port'] = 22
unicorn['worker_timeout'] = 60
unicorn['worker_processes'] = 3
